<?xml version = "1.0" encoding = "UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="engine1" class="com.yash.springioc.que6.Engine">
		<property name="engine_type" value="single cylinder"></property>
		<property name="engine_power" value="300"></property>
		<property name="engine_name" value="merz"></property>

	</bean>

<bean id="engine2" class="com.yash.springioc.que6.Engine">
		<property name="engine_type" value="double cylinder"></property>
		<property name="engine_power" value="500"></property>
		<property name="engine_name" value="volkswagon"></property>

	</bean>

<bean id="engine3" class="com.yash.springioc.que6.Engine">
		<property name="engine_type" value="normalengine"></property>
		<property name="engine_power" value="150"></property>
		<property name="engine_name" value="YamahaEngine"></property>

	</bean>
	<bean id="vehicle1" class="com.yash.springioc.que6.Vehicle">
		<property name="vehicle_name" value="Mercedes"></property>
		<property name="vehicle_type" value="four wheeler"></property>
		<property name="engineObj" ref="engine1"></property>
		<property name="mileage" value="10km"></property>
		<property name="price" value="9000000"></property>
	</bean>
	
	
		<bean id="vehicle2" class="com.yash.springioc.que6.Vehicle">
		<property name="vehicle_name" value="Fortuner"></property>
		<property name="vehicle_type" value="four wheeler"></property>
		<property name="engineObj" ref="engine2"></property>
		<property name="mileage" value="15km"></property>
		<property name="price" value="3550000"></property>
	</bean>

	<bean id="vehicle3" class="com.yash.springioc.que6.Vehicle">
		<property name="vehicle_name" value="FZS-Verion3"></property>
		<property name="vehicle_type" value="Two wheeler"></property>
		<property name="engineObj" ref="engine3"></property>
		<property name="mileage" value="50km"></property>
		<property name="price" value="160000"></property>
	</bean>



</beans>